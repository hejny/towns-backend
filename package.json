{
  "name": "towns5api",
  "version": "0.1.1",
  "description": "API SERVER for Towns 5",
  "private": true,
  "scripts": {
    "rebuild": "node-gyp rebuild",
    "mongo-migrate": "node ./node_modules/mongodb-migrate -runmm -cfg ./config/mongo.json -dbn mongoDb up",
    "mongo-remigrate": "node ./node_modules/mongodb-migrate -runmm -cfg ./config/mongo.json -dbn mongoDb down && node ./node_modules/mongodb-migrate -runmm -cfg ./config/mongo.json -dbn mongoDb up",
    "mongo-test-remigrate": "node ./node_modules/mongodb-migrate -runmm -cfg ./config/mLab.json -dbn mongoDb down && node ./node_modules/mongodb-migrate -runmm -cfg ./config/mLab.json -dbn mongoDb up",
    "start": "node ./bin/www",
    "start-test": "NODE_ENV=test node ./bin/www",
    "start-debug": "DEBUG=api:* node ./bin/www",
    "nodemonitor": "DEBUG=api:* nodemon ./bin/www",
    "mocha": "make test",
    "test": "NODE_ENV=test make test"
  },
  "author": "Stefan Kecskes <mr.kecskes@gmail.com> (https://skey.uk)",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^0.8.5",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "jwt-simple": "^0.4.1",
    "lodash": "^4.6.1",
    "mongodb": "~2.1.2",
    "mongodb-migrate": "~1.2.3",
    "mongoose": "~4.2.5",
    "mongoose-double": "0.0.1",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0",
    "validator": "~4.5.2"
  },
  "devDependencies": {
    "gulp": "^3.9.0",
    "gulp-jshint": "^2.0.0",
    "jshint": "^2.9.1",
    "mocha": "^2.4.5",
    "nodemon": "~1.8.1",
    "should": "^8.2.1",
    "supertest": "^1.1.0",
    "gulp-env": "~0.4.0"
  }
}
